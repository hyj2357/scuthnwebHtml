	  function clickFI($this){
		  var  e = $this.css("right");
		  var _e = parseFloat(e.substring(0,e.length-2));
		  if(_e>=0)
			 $(".floatInfo:hover").css("right","-190px");
		  else
			 $(".floatInfo:hover").css("right","0%");
		  $this.css("transition","right 0.2s");
		  $this.css("-moz-transition","right 0.2s"); /* Firefox 4 */
		  $this.css("-webkit-transition","right 0.2s"); /* Safari and Chrome */
		  $this.css("-o-transition","right 0.2s"); /* Opera */ 
		  console.info("click...");
		  //$this.html($this.css("left"));
	}
